{
  "name": "@cycjimmy/h5-pages",
  "version": "3.2.0",
  "description": "full h5 page plugin based on swiper",
  "type": "module",
  "main": "dist/h5-pages.cjs.js",
  "module": "dist/h5-pages.esm.js",
  "browser": "dist/h5-pages.umd.min.js",
  "scripts": {
    "start": "npm run dev",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --color --forceExit --detectOpenHandles",
    "coveralls": "NODE_OPTIONS=--experimental-vm-modules jest --color --forceExit --detectOpenHandles --coverage",
    "lint": "eslint --ext .js,.cjs,.mjs -c .eslintrc .",
    "lint:fix": "eslint --ext .js,.cjs,.mjs --fix -c .eslintrc . --fix",
    "dev": "cross-env NODE_ENV=development rollup -c rollup/rollup.config.dev.cjs --watch",
    "build": "npm run build:prod:umd && npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production rollup -c rollup/rollup.config.prod.cjs",
    "build:prod:umd": "cross-env NODE_ENV=production rollup -c rollup/rollup.config.dev.cjs",
    "build:deployment": "cross-env NODE_ENV=deployment rollup -c rollup/rollup.config.dev.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycjimmy/h5-pages.git"
  },
  "keywords": [
    "full",
    "h5",
    "page",
    "swiper"
  ],
  "author": "cycjimmy <cycjimmy@gmail.com> (https://github.com/cycjimmy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cycjimmy/h5-pages/issues"
  },
  "homepage": "https://github.com/cycjimmy/h5-pages#readme",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@cycjimmy/awesome-js-funcs": "^4.0.1",
    "@cycjimmy/sass-lib": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.16.11",
    "@cycjimmy/config-lib": "^3.0.4",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "autoprefixer": "^10.4.3",
    "babel-jest": "^27.5.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-rollup": "^0.1.0",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "rollup": "^2.70.1",
    "rollup-plugin-browsersync": "^1.3.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "swiper": "^8.0.7"
  }
}
